<!--pages/home/childCompon/z-recommend/z-recommend.wxml-->
<view class="recommend">
	<block wx:for="{{recommends}}" wx:key="index">
		<view class="recommend-item">
			<view>
				<image src="{{item.image}}" mode="widthFix" class="recommend-item-image"></image>
			</view>
			<text class="recommend-item-text">{{item.title}}</text>
		</view>
	</block>
</view>