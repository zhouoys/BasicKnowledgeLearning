<!--pages/detail/childCompon/z-detailInfo/z-detailInfo.wxml-->
<view class="detail-info">
	<view class="detail-info-header">
		<view class="line start"></view>
		<view class="header-desc">{{detailInfo.desc}}</view>
		<view class="line end"></view>
	</view>
	<view class="content">
		<block wx:for="{{detailInfo.detailImage}}" wx:for-item="outerItem" wx:for-index="outerIndex" wx:key="outerIndex">
			<view class="title">{{outerItem.key}}</view>
			<block wx:for="{{outerItem.list}}" wx:for-item="innerItem" wx:for-index="innerIndex" wx:key="innerIndex">
      <image src="{{innerItem}}" mode="widthFix" class="image"></image>
			</block>
		</block>
	</view>
</view>